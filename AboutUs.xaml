            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" TextAlignment="Center" FontSize="50" FontWeight="Black">Airline Information System</TextBlock>
    </Grid>
</Window>
